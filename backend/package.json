{
  "name": "backend",
  "version": "1.0.0",
  "description": "business api",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "cross-env ENV_FILE=./config/.env.prod NODE_ENV=production node -r tsconfig-paths/register ./dist/app.js",
    "dev": "cross-env ENV_FILE=./config/.env.dev NODE_ENV=development ts-node -r tsconfig-paths/register --files ./src/app.ts",
    "test:unit": "cross-env ENV_FILE=./config/.env.test jest --verbose --silent=false",
    "test:coverage": "cross-env ENV_FILE=./config/.env.test jest --coverage",
    "migrate:postgres:dev": "dotenv -e ./config/.env.dev -- npx prisma migrate dev",
    "migrate:postgres:test": "dotenv -e ./config/.env.test -- npx prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed:dev": "dotenv -e ./config/.env.dev -- ts-node --transpile-only ./prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splotnikov-sketch/businesses.git"
  },
  "author": "Sergey Plotnikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/splotnikov-sketch/businesses/issues"
  },
  "homepage": "https://github.com/splotnikov-sketch/businesses#readme",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.13",
    "@types/validator": "^13.7.10",
    "@types/yamljs": "^0.2.31",
    "add": "^2.0.6",
    "axios": "^1.2.1",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.8",
    "node-cache": "^5.1.2",
    "swagger-routes-express": "^3.3.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4",
    "validator": "^13.7.0",
    "winston": "^3.8.2",
    "yamljs": "^0.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/jest": "^29.2.4",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "prisma": "^4.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  }
}
